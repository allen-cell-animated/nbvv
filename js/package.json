{
  "name": "nbvv",
  "version": "1.3.2",
  "description": "NoteBook Volume Viewer component for jupyter extension",
  "author": "aics",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allen-cell-animated/nbvv.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run build:lib && npm run build:css && npm run build:labextension && webpack --mode=production",
    "build:lib": "tsc --project .",
    "build:css": "lessc --js --include-path=./node_modules src/style.less lib/style.css",
    "build:labextension": "jupyter labextension build .",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w --project .",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch .",
    "watch:dev": "npm-run-all -p watch:lib watch:nbextension"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../share/jupyter/labextensions/nbvv",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@jupyterlab/builder": "^3.4.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^6.2.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "resolve-url-loader": "^3.1.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@aics/web-3d-viewer": "^2.2.0",
    "@jupyter-widgets/base": "^6.0.1",
    "antd": "^3.20.7",
    "chokidar": "^3.4.1",
    "lodash": "^4.17.4",
    "sass": "^1.26.10"
  }
}
